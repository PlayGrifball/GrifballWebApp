{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Debug" ],
    "Enrich": [ "FromLogContext" ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "GrifballWebApp": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      "Console",
      "Debug"
    ]
  }
}
